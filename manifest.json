{
  "manifest_version": 3,
  "name": "APQ Debugger (Beta)",
  "description": "This extension intercepts Apollo's persisted queries, forcing it to retry the request by sending the entire graphql query.",
  "version": "0.0.8",
  "permissions": ["debugger", "tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "./js/service-worker.js"
  },
  "devtools_page": "./frontend/devtools.html",
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  }
}